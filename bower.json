    "highcharts": {
      "main": [
        "highcharts.js",
        "highcharts-more.js",
        "/modules/treemap.js",
        "/modules/broken-axis.js"
      ]
    }
  },
  "resolutions": {
    "tableExport.jquery.plugin": "~1.6.8"
  }
}
