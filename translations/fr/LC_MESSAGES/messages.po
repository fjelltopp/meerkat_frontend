#: meerkat_frontend/templates/reports/email/plain/email_ncd_report_plain.html:30
#, python-format
msgid "%(d_control)s%% diabetes patients controlled (HbA1C < 7%%)."
msgstr "%(d_control)s patients atteints de diabète contrôlés (HbA1c <7%%)."

#: meerkat_frontend/templates/reports/email/plain/email_pip_plain.html:1
#, python-format
msgid ""
"Dear Colleagues,\n"
"  Welcome to the latest Pandemic Influenza Preparedness (PIP) Report from"
" the Public Health Surveillance project in %(project_region)s."
msgstr ""
"Chers collègues, \n"
" Bienvenue sur le dernier rapport de préparation à la pandémie de grippe "
"(PIP) du projet de surveillance de la santé publique à "
"%(project_region)s."

#: meerkat_frontend/templates/reports/email/plain/email_pip_plain.html:18
#, python-format
msgid "A total of %(cases)s enrolled patients."
msgstr "Au total %(cases)s patients inscrits."

