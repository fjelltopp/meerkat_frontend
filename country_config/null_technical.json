        "all": "7",
        "cd": "4",
        "ncd": "4",
        "mh": "4"
    },

    "timeliness_denominator": {
        "all": "4",
        "cd": "4",
        "ncd": "4",
        "mh": "4"
    },

    "weekend" : [5,6],

