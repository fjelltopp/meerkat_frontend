        "types": ["incidence_clinic", "incidence_district", "incidence_region", "cases_clinic", "cases_district", "cases_region"]

    },

    "indicators":{
        "list":[
            {"name":"Number of female patients","call":{"flags":["n"], "variables":["gen_1"]}},
            {"name":"Number of male patients","call":{"flags":["n"], "variables":["gen_2"]}},
            { "name":"% Male", "call":{"flags":["d","n","m"], "variables":["tot_1","gen_1", "100"]} }
         ]
    },

