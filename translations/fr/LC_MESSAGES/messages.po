#: meerkat_frontend/templates/reports/email/plain/email_ncd_report_plain.html:1
#, python-format
msgid ""
"Dear Colleagues,\n"
"  Welcome to the latest Non-Communicable Disease Epidemiological Report "
"from the Public Health Surveillance project in %(project_region)s."
msgstr ""
"Chers collègues, \n"
" \n"
" Bienvenue sur le rapport épidémiologique des maladies non-transmissibles"
" du projet de Surveillance de la Santé Publique à %(project_region)s."

#: meerkat_frontend/templates/reports/email/plain/email_ncd_report_plain.html:18
#, python-format
msgid "A total of %(hypertension)s hypertension cases."
msgstr "Un total de %(hypertension)s cas d'hypertension."

#: meerkat_frontend/templates/reports/email/plain/email_ncd_report_plain.html:22
#, python-format
msgid "%(ht_control)s%% hypertensive patients controlled (BP < 140/90)."
msgstr "%(ht_control)s %% patients hypertendus contrôlés (BP <140/90)."

