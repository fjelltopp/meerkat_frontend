msgid ""
"Dear Colleagues,\n"
"  Welcome to the latest malaria surveillance report from the Public "
"Health Surveillance project in %(project_region)s."
msgstr ""
"Chers collègues,\n"
" Bienvenue sur le Rapport de Surveillance Epidemiologique du Paludisme "
"par le projet de Surveillance de la Santé Publique à %(project_region)s."

#: meerkat_frontend/templates/reports/email/plain/email_malaria_plain.html:18
#, python-format
msgid ""
"A total of %(consultations)s consultations reported from %(clinics)s "
"health facilities."
msgstr ""
"Au total %(consultations)s consultations par %(clinics)s établissements "
"de santé."

#: meerkat_frontend/templates/reports/email/plain/email_malaria_plain.html:23
#, python-format
msgid ""
"%(fever)s cases and %(rdt)s RDT tests \n"
" performed."
msgstr "%(fever)s cas de fièvre et %(rdt)s TDR effectués."

#: meerkat_frontend/templates/reports/email/plain/email_malaria_plain.html:29
#, python-format
msgid ""
"%(simple_malaria)s cases of \n"
"simple malaria and %(severe_malaria)s\n"
"cases of severe malaria"
msgstr ""
"%(simple_malaria)s cas de Paludisme simple et %(severe_malaria)s\n"
"cas de Paludisme grave"

#: meerkat_frontend/templates/reports/email/plain/email_malaria_plain.html:37
#, python-format
msgid "%(mal_mortality)s malaria deaths"
msgstr "%(mal_mortality)s décès dus au paludisme"

